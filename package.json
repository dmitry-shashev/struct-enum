{
  "name": "struct-enum",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitry-shashev/struct-enum"
  },
  "version": "0.0.9",
  "description": "Improved constants",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "shx rm -rf dist",
    "build": "tsc --module CommonJS --outDir dist/cjs",
    "prettier-format": "prettier --write . && git add --all",
    "test": "tsx ./test/main.test.ts",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.1",
    "pnpm": ">=8.7.4"
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "packageManager": "pnpm@8.7.4",
  "keywords": [
    "enum",
    "constant",
    "struct"
  ],
  "author": {
    "name": "Dmitry",
    "email": "dy.shashev@gmail.com",
    "url": "https://github.com/dmitry-shashev"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.7.0",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "shx": "0.3.4",
    "tsx": "4.19.1",
    "typescript": "5.2.2",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
